package com.liyang.dto;

import java.math.BigDecimal;
import java.util.Date;

public class PaymentingInfo {

	private BigDecimal principal; // 还款本金
	private BigDecimal interinest; // 还款利息
	private BigDecimal paymantRemain;// 还款余额
	private String payDate; // 还款日期
	private Integer loanoverdueDays; // 逾期天数
	private BigDecimal punishInterinest;// 罚息
	private Integer payof; // 是否还款
	private Integer overdue; //是否逾期
	private BigDecimal realPayAmount;//实际还款
	
	
	

	public BigDecimal getRealPayAmount() {
		return realPayAmount;
	}

	public void setRealPayAmount(BigDecimal realPayAmount) {
		this.realPayAmount = realPayAmount;
	}

	public Integer getOverdue() {
		return overdue;
	}

	public void setOverdue(Integer overdue) {
		this.overdue = overdue;
	}

	public BigDecimal getPrincipal() {
		return principal;
	}

	public void setPrincipal(BigDecimal principal) {
		this.principal = principal;
	}

	public BigDecimal getInterinest() {
		return interinest;
	}

	public void setInterinest(BigDecimal interinest) {
		this.interinest = interinest;
	}

	public BigDecimal getPaymantRemain() {
		return paymantRemain;
	}

	public void setPaymantRemain(BigDecimal paymantRemain) {
		this.paymantRemain = paymantRemain;
	}

	public String getPayDate() {
		return payDate;
	}

	public void setPayDate(String payDate) {
		this.payDate = payDate;
	}

	public Integer getLoanoverdueDays() {
		return loanoverdueDays;
	}

	public void setLoanoverdueDays(Integer loanoverdueDays) {
		this.loanoverdueDays = loanoverdueDays;
	}

	public BigDecimal getPunishInterinest() {
		return punishInterinest;
	}

	public void setPunishInterinest(BigDecimal punishInterinest) {
		this.punishInterinest = punishInterinest;
	}

	public Integer getPayof() {
		return payof;
	}

	public void setPayof(Integer payof) {
		this.payof = payof;
	}

}
