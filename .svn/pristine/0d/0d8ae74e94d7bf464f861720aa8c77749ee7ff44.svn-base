                                                <!--<button ng-if="state_code == 'NOTLEND'" ng-click="fn.adopt(item.id)" class="pull-left btn btn-sm btn-success m-r">放款</button>-->
                                                <!--<button ng-if="mdbtBtn"  ng-click="fn.noPass(item.id)" class="pull-left btn btn-sm btn-danger m-r">未通过</button>-->
                                                <!--<button ng-if="mdbtBtn" ng-click="fn.upload(item.id)"  class="pull-left btn btn-sm btn-primary m-r">上传凭证</button>-->
                                                <!--<button ng-if="!(state_code =='PAYOF')" ng-click="fn.openFile(item.id)" class="pull-left btn btn-sm btn-info m-r">查看凭证</button>-->
                                                <!--<button ui-sref="workflowEntity.home.profile.page({entity_key:'mdbtloan',view_type:'firstphase',id:item.id})"  ng-if="(state_code == 'ENABLED')||state_code == 'PAYOF'" class="pull-left btn btn-sm btn-success m-r">详情</button>-->
                                                <!--&lt;!&ndash;<button ui-sref="workflowEntity.home.profile.form({id:item.id})"&ndash;&gt;-->
