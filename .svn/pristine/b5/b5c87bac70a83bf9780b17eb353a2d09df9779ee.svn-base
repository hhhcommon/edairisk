<script src="https://res.wx.qq.com/open/js/jweixin-1.2.0.js"></script>
<script src="../../../js/loading.js"></script>
<script src="../../../js/picker.js"></script>
<script src="../../../js/picker-city.js"></script>
<script>
    //上传图片
    var arrData=[];
    //jsSDK
    var JsUrl =window.location.href;
    $.ajax({
        type: "get",
        url: "/wechat/wlqz/getCofigure",
        data:{'JsUrl':JsUrl},
        dataType: "json",
        success: function(res){
            console.log(res)
            wx.config({
                debug: false,
                appId:res.appId ,
                timestamp: res.timestamp,
                nonceStr: res.nonceStr,
                signature: res.signature,
                jsApiList: [
                    'chooseImage',
                    'uploadImage',
                    'previewImage',
                    'getLocation'
                    // 所有要调用的 API 都要加到这个列表中
                ]
            });
            wx.ready(function () {
                wx.getLocation({
                    type: 'wgs84', // 默认为wgs84的gps坐标，如果要返回直接给openLocation用的火星坐标，可传入'gcj02'
                    success: function (res) {
                        var latitude = res.latitude; // 纬度，浮点数，范围为90 ~ -90
                        var longitude = res.longitude; // 经度，浮点数，范围为180 ~ -180。
                        var speed = res.speed; // 速度，以米/每秒计
                        var accuracy = res.accuracy; // 位置精度
                        console.log(latitude);
                        console.log(longitude);
                        //设置AJAX的全局默认选项
                        $.ajaxSetup( {

                            headers: { // 默认添加请求头
                                app_code:"WULIUQIANZHUANG",
                                client:"wechat",
                                latitude:latitude,
                                longitude:longitude
                            }
                        } );

                    }
                });



            });
            wx.error(function(res){

                // config信息验证失败会执行error函数，如签名过期导致验证失败，具体错误信息可以打开config的debug模式查看，也可以在返回的res参数中查看，对于SPA可以在这里更新签名。
            });


        }
    })

    if(window.location.search){
        var href = window.location.href.split('?')[1];
        var splitId = href.split('&')[0];
        var splitProduct= href.split('&')[1];
        window.id = splitId.split('=')[1];    //产品实体ID
        window.product= splitProduct.split('=')[1];  //哪种产品


            //填充信息
            $.ajax({
                type: 'GET',
                url: '/rest/orderrzzls/'+window.id ,
                data: {} ,
                headers: {
                    app_code:"WULIUQIANZHUANG",
                    client:"wechat"
                },
                success: function (res) {
                    console.log(res.comment)
                    $('#comment').text(res.comment);
                    $('#realName').val(res.realName);
                    $('#applyMobile').val(res.applyMobile);
                    $('#applyIdentityNo').val(res.applyIdentityNo);
                    $('#applyEnterpriseName').val(res.applyEnterpriseName);
                    $('#applyEnterpriseReigionName').val(res.applyEnterpriseProvince+' '+res.applyEnterpriseCity+' '+res.applyEnterpriseDistrict);

                } ,
                dataType: 'json'
            });


            //填充图片
            $.ajax({
                type: 'get',
                url: '/rest/orderrzzls/'+window.id+'/files' ,
                data:{},
                headers: {
                    app_code:"WULIUQIANZHUANG",
                    client:"wechat",
                    "Content-Type":"application/json",
                },
                success: function (res) {
                    var ImgData=res._embedded.orderrzzlFiles;
                    $.each(ImgData,function (key,val) {
                        if(val.subcategory == '营业执照'){
                            $('#yyzz').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }else if(val.subcategory == '开户许可证'){
                            $('#khxk').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '快递经营许可证'){
                            $('#kdjy').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '公司章程'){
                            $('#gszc').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '网点加盟协议'){
                            $('#wdjm').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '租房合同'){
                            $('#zfht').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '法人身份证原件正反面照片'){
                            $('#frdb').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '夫妻双方身份证正反面照片'){
                            $('#fqsf').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '结婚证照片'){
                            $('#zhz').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
                        }
                        else if(val.subcategory == '征信报告(企业及法人夫妻)'){
                            $('#zxbg').prepend('<div style="position: relative;display: inline-block"><img class="imgBtn" src="http://files.xiaojinpingtai.com/'+val.newFileName+'" upsrc="null" style="width: 67px; height: 67px;margin: 0 5px;"></div>')
