          	        console.log(longitude);
          	   			 //设置AJAX的全局默认选项  
		          	      $.ajaxSetup( {  
		          	         
		          	          headers: { // 默认添加请求头  
		          	        	 app_code:"WULIUQIANZHUANG",
	          	           		 client:"wechat",
	          	           		 latitude:latitude,
          	           			 longitude:longitude
		          	          }  
		          	      } ); 
          	        
          	    }
          	});
        	  
          	

