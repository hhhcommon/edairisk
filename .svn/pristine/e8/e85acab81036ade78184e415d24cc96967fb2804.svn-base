"use strict";app.filter("fromNow",function(){return function(a){return moment(a).fromNow()}}).filter("datetime",function(){return function(a){if(parseInt(a)>1400000){a=(a+"000")*1}return moment(a).format("YYYY")+"年 "+moment(a).format("MM")+"月"+moment(a).format("DD")+"日 "+moment(a).format("HH:mm")}}).filter("toFixed",function(){return function(a){return a.toFinite(2)}}).filter("toLocalDate",function(){return function(a){if(a){if(parseInt(a)>1400000){a=(a+"000")*1}return moment(a).format("YYYY MMMDo")}else{return"未填写或未知"}}}).filter("myJson",function(){return function(a){if(a){return JSON.parse(a)}else{return"无"}}}).filter("nullToString",function(){return function(a){if(a){return a}else{return"未填写或未知"}}});